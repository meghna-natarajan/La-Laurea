<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gradday">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegistrationActivity" android:label="La Laurea"> </activity>
        <activity android:name=".HomeActivity" android:label="La Laurea"> </activity>
        <activity android:name=".AgendaActivity" android:label="La Laurea"> </activity>
        <activity android:name=".Rsvp" android:label="La Laurea"> </activity>
        <activity android:name=".NoteActivity" android:label="La Laurea"> </activity>
        <activity android:name=".eInviteActivity" android:label="La Laurea"> </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>
